
<h1>Listing contracts</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Date</th>
      <th>Location</th>
      <th>Violence Required</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contracts.each do |contract| %>
      <tr>
        <td><%= contract.name %></td>
        <td><%= link_to "Show", contract_path(contract), class:"btn btn-secondary my-2" %></td>

      </tr>
    <% end %>
  </tbody>
</table>

<br>


